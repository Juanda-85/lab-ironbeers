<div>

    {{#each randomBeer}}
    <h3>{{this.name}}</h3>
    <img src="{{this.image_url}}" alt="">
    <p>Description = "{{this.description}}"</p>
    <p>Tagline = "{{this.tagline}}</p>
    <p>Food Pairing = "{{this.food_pairing}}"</p>
    <p>Brewer Tips = "{{this.brewers_tips}}"</p>



    {{/each}}
</div>